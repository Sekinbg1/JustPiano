<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KeyboardModeView">
        <attr name="whiteKeyNum" format="integer" />
        <attr name="whiteKeyOffset" format="integer" />
    </declare-styleable>

    <declare-styleable name="DataSelectView">
        <attr name="dataEditable" format="boolean" />
        <attr name="dataTextSize" format="integer" />
        <attr name="dataTextColor" format="color" />
        <attr name="dataNameList" format="reference" />
        <attr name="dataValueList" format="reference" />
        <attr name="dataDefaultName" format="string" />
        <attr name="dataOnlyNumber" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GoldConvertView">
        <attr name="stepChangeValue" format="float" />
        <attr name="topGoldConvertText" format="string" />
        <attr name="bottomGoldConvertText" format="string" />
        <attr name="defaultValue" format="float" />
    </declare-styleable>
</resources>