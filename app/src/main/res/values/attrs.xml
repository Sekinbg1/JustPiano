<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KeyboardView">
        <attr name="whiteKeyNum" format="integer" />
        <attr name="whiteKeyOffset" format="integer" />
        <attr name="pianoKeyTouchable" format="boolean" />
        <attr name="octaveTagType" format="integer" />
    </declare-styleable>

    <declare-styleable name="DataSelectView">
        <attr name="dataEditable" format="boolean" />
        <attr name="dataTextSize" format="integer" />
        <attr name="dataTextColor" format="color" />
        <attr name="dataTextBackground" format="reference" />
        <attr name="dataNameList" format="reference" />
        <attr name="dataValueList" format="reference" />
        <attr name="dataDefaultName" format="string" />
        <attr name="dataOnlyNumber" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GoldConvertView">
        <attr name="textBackground" format="reference" />
        <attr name="stepChangeValue" format="float" />
        <attr name="topGoldConvertText" format="string" />
        <attr name="bottomGoldConvertText" format="string" />
        <attr name="defaultActualValue" format="float" />
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="minValue" format="float" />
        <attr name="maxValue" format="float" />
        <attr name="critValue" format="float" />
        <attr name="floatNumber" format="boolean" />
        <attr name="maxSteps" format="integer" />
    </declare-styleable>
</resources>